# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_BACKEND_URL=http://localhost:5000

# Google OAuth Configuration
# Replace this with your actual Google OAuth Client ID from Google Cloud Console
# This should be the same Client ID as used in the backend
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here

# App Configuration
REACT_APP_NAME=Media Gallery
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_GOOGLE_OAUTH=true
REACT_APP_ENABLE_EMAIL_VERIFICATION=true
REACT_APP_ENABLE_PASSWORD_RESET=true

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# UI Configuration
REACT_APP_DEFAULT_PAGE_SIZE=12
REACT_APP_MAX_PAGE_SIZE=50
REACT_APP_SEARCH_DEBOUNCE_MS=300

# Development Configuration
REACT_APP_ENABLE_DEBUG_LOGS=true
REACT_APP_ENABLE_MOCK_DATA=false

# Authentication Configuration
REACT_APP_JWT_STORAGE_KEY=media_gallery_token
REACT_APP_REFRESH_TOKEN_KEY=media_gallery_refresh_token

# Error Handling
REACT_APP_SHOW_ERROR_DETAILS=true
REACT_APP_LOG_ERRORS_TO_CONSOLE=true 